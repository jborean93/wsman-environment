#!/usr/bin/env bash

[ ! -d "cp" ] && mkdir cp

sshpass \
    -p vagrant \
    ssh \
    -o "ControlMaster=auto" \
    -o "ControlPersist=no" \
    -o "ControlPath=cp/ssh-%r@%h:%p" \
    -o "StrictHostKeyChecking=no" \
    -o "UserKnownHostsFile=/dev/null" \
    -o "LogLevel=ERROR" \
    -CfNq \
    -D 127.0.0.1:{{ socks_port }} \
    -p 22 \
    vagrant@squid.{{ domain_realm }}

